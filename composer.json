{
    "name": "bshirey/hierarchical-config",
    "description": "Provides inheritance and hierarchical configuration tools.",
    "authors": [
        {
            "name": "Brian Shirey",
            "email": "brian.shirey@atlanticbt.com"
        }
    ],
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "phpunit/phpunit": "*",
        "zendframework/zend-config": "2.3.*@dev",
        "symfony/class-loader": "3.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "HierarchicalConfig\\": "src/",
            "HierarchicalConfig\\Config\\": "src/config/",
            "HierarchicalConfig\\Writer\\": "src/writer/",
            "HierarchicalConfig\\Helper\\": "src/helper/",
            "HierarchicalConfig\\Filter\\": "src/filter/",
            "HierarchicalConfig\\Stub\\": "src/stub/"
        }
    }
}
